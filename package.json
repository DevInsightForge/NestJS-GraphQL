{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "rimraf dist && nest build",
    "migration:generate": "typeorm-ts-node-esm -d ./src/configs/typeorm.config.ts migration:generate -p ./src/migrations/schema",
    "migration:migrate": "typeorm-ts-node-esm -d ./src/configs/typeorm.config.ts migration:run",
    "migration:revert": "typeorm-ts-node-esm -d ./src/configs/typeorm.config.ts migration:revert",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@apollo/server": "4.4.1",
    "@nestjs/apollo": "^11.0.4",
    "@nestjs/common": "9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.3.9",
    "@nestjs/graphql": "11.0.0",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "graphql": "16.6.0",
    "graphql-query-complexity": "0.12.0",
    "graphql-subscriptions": "2.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@nestjs/cli": "9.2.0",
    "@types/express": "4.17.17",
    "@types/node": "18.15.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "dotenv": "^16.0.3",
    "eslint": "8.35.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.4",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  }
}
